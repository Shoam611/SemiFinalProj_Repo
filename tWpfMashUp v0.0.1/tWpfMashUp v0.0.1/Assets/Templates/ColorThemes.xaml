<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Primery">#292b2f</Color>
    <SolidColorBrush x:Key="PrimeryBrush" Color="{StaticResource Primery}" />

    <Color x:Key="Accent">#2f3136</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}" />

    <Color x:Key="Complimentary">#36393f</Color>
    <SolidColorBrush x:Key="ComplimentaryBrush" Color="{StaticResource Complimentary}" />
    <!--
    mains:
    Color="#292b2f" - Primery
    Color="#2f3136" - Accent
    Color="#36393f" - Complimentary
    
    from chat thread
    Color="#2f3139"
    
    chat app current background
    Color="#36393f" - Complimentary
    online/new message circle
    Color ="#3bff6f" - green/active
    passive
    Color ="CornflowerBlue" - blue/passive
    
    
    -->
</ResourceDictionary>