<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Primery">#292b2f</Color>
    <SolidColorBrush x:Key="PrimeryBrush" Color="{StaticResource Primery}" />

    <Color x:Key="Accent">#2f3136</Color>
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource Accent}" />

    <Color x:Key="Complimentary">#36393f</Color>
    <SolidColorBrush x:Key="ComplimentaryBrush" Color="{StaticResource Complimentary}" />

    <Color x:Key="Dim">#252525</Color>
    <SolidColorBrush x:Key="DimBrush" Color="{StaticResource Dim}" />

    <Color x:Key="Active">#3bff6f</Color>
    <SolidColorBrush x:Key="ActiveBrush" Color="{StaticResource Active}" />
    
    <!--
    mains:
    Color="#292b2f" - Primery
    Color="#2f3136" - Accent
    Color="#36393f" - Complimentary
    
    from chat thread
    Color="#2f3139"
    
    from chat app
    Color="#36393f" - Complimentary
    online/new message circle
    Color ="#3bff6f" - green/active
    passive
    Color ="CornflowerBlue" - blue/passive
    contatct active bg Color = "#252525" - Active
    contatct passive bg Color ="#323b40" - Passive #323b40

    -->
</ResourceDictionary>